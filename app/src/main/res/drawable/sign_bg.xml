<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="96dp"
    android:height="96dp"
    android:viewportHeight="100"
    android:viewportWidth="100">

    <path
        android:pathData="M 0,0 L 100,0 100,100 0,100 z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:startX="50"
                android:startY="1"
                android:endX="50"
                android:endY="99">
                <item android:color="@color/blue_light" android:offset="0" />
                <item android:color="@color/blue" android:offset="0.34" />
                <item android:color="@color/blue_dark" android:offset="0.66" />
                <item android:color="@color/purple" android:offset="1" />
            </gradient>
        </aapt:attr>
    </path>
</vector>